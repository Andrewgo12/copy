MEMORY_LIMIT_MB=1024
EMAIL_BATCH_SIZE=50

# Connection settings
CONNECTION_TIMEOUT=30
READ_TIMEOUT=60
ENABLE_SSL_VERIFICATION=true

# Output settings
OUTPUT_FORMAT=json
COMPRESSION_ENABLED=false
ENCRYPTION_ENABLED=false
DEBUG_MODE=false
