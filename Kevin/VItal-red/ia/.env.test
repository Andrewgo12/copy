# Test configuration for Gmail processing
GMAIL_EMAIL=kevinrlinze@gmail.com
GMAIL_PASSWORD=test_password_placeholder

# Base configuration
BASE_PATH=.
LOG_LEVEL=INFO
MAX_EMAILS=10
PROCESSING_TIMEOUT=300

# Feature flags
ENABLE_OCR=true
ENABLE_TEXT_EXTRACTION=true
ENABLE_JSON_OUTPUT=true
ENABLE_DETAILED_LOGGING=true
BACKUP_ENABLED=false
CLEANUP_TEMP_FILES=true

# Performance settings
PARALLEL_PROCESSING=false
