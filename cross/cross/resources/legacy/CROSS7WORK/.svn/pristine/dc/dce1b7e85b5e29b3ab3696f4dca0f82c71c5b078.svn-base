require_once "Web/WebRequest.class.php";

Class FeCuCmdDefaultSolicitante {

	function execute() {
		extract($_REQUEST);
		
		settype($objManager, "object");
		settype($rcUser, "array");
		print("A");
		$rcUser = Application::getUserParam();
		
		if($_REQUEST["lang"])
			$rcUser["lang"] = $_REQUEST["lang"];
		else if($_SESSION["_authsession"]["lang"])
			$rcUser["lang"] = $_SESSION["_authsession"]["lang"];
		WebSession :: setProperty("_authsession", & $rcUser);
		print("B");
		
		if(isset($web))
			WebSession :: setProperty("web", & $web);
		
		//SI limpia el $_REQUEST
		if ($clean_table) {
			print("C");

			$objManager = Application :: getDomainController('SolicitanteManager');
			$objManager->UnsetRequest();
			unset ($_REQUEST["clean_table"]);
			unset($_REQUEST["typesol"]);
		}
		if ((WebSession :: issetProperty("_rcSolicitante"))) {
			print("D");
			WebSession :: unsetProperty("_rcSolicitante");	
		}
		
		return "success";
	}

}
?>