DIV.content {
	BACKGROUND-IMAGE: url(../images/trees/tree-mientrasTanto/left_line.gif); 
	BACKGROUND-REPEAT: repeat-y; 
	HEIGHT: 90%; 
	BACKGROUND-COLOR: #efefef
}
DIV.boxcontrol {
	PADDING-RIGHT: 1px; 
	BACKGROUND-POSITION: right 50%; 
	PADDING-LEFT: 15px; 
	BACKGROUND-IMAGE: url(../images/trees/tree-mientrasTanto/right_line.gif); 
	PADDING-BOTTOM: 0px; 
	PADDING-TOP: 0px; 
	BACKGROUND-REPEAT: repeat-y; 
	HEIGHT: 100%
}
FIELDSET.context_help {
	BACKGROUND: #ffffff; 
