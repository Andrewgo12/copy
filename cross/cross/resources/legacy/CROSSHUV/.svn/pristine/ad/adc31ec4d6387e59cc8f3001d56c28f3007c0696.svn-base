		$rcLeftJoin["ordenempresa.priocodigos"] = array ("view" => '"prioridad"."prionombres" AS "priocodigos"', "where" => '"ordenempresa"."priocodigos"="prioridad"."priocodigos"', "table" => 'prioridad',"source"=>'"ordenempresa"');
