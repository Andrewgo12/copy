		$rcLeftJoin["paciente.pacihisclis"] = array ("view" => '"paciente"."pacihisclis"', "where" => '"ordenempresa"."paciindentis"="paciente"."paciindentis"', "table" => 'paciente',"source"=>'"ordenempresa"');

		$rcTables['"orden"'] = null;
		$rcTables['"ordenempresa"'] = null;

