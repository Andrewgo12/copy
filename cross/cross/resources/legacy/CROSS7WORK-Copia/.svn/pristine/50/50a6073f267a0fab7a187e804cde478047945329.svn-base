BODY {
	FONT-SIZE: 10px; margin-left: 0px; margin-top: 0px;	margin-right: 0px; margin-bottom: 0px; COLOR: #666666; FONT-FAMILY: Arial, Helvetica, sans-serif
}
TD {
	BACKGROUND: #e8f5ff; FONT: 10px Arial, Helvetica, sans-serif; TEXT-TRANSFORM: none; COLOR: #1C4AB9; TEXT-DECORATION: none
}
TH {
	BACKGROUND-COLOR: #FFFFFF; FONT-WEIGHT: bold; FONT-SIZE: 16px; COLOR: #1C4AB9; FONT-FAMILY: Arial, Helvetica, sans-serif; ALIGN: left	
}
SELECT {
	FONT-SIZE: 10px; COLOR: #1c4ab9; FONT-FAMILY: Arial, Helvetica, sans-serif
}
INPUT {
	FONT-SIZE: 10px; COLOR: #1c4ab9; FONT-FAMILY: Arial, Helvetica, sans-serif
}
TEXTAREA{
	FONT-SIZE: 10px; COLOR: #1c4ab9; FONT-FAMILY: Arial, Helvetica, sans-serif
}
.titulofila {
	FONT-SIZE: 11px; BACKGROUND: #1c49bc; COLOR: #ffffff; FONT-FAMILY: Arial, Helvetica, sans-serif
}
.celda2 {
	BACKGROUND: #ffffff; FONT: 12px Arial, Helvetica, sans-serif; TEXT-TRANSFORM: none; COLOR: #666666; TEXT-DECORATION: none
}
.celda {
	BACKGROUND: #e8f5ff; FONT: 12px Arial, Helvetica, sans-serif; TEXT-TRANSFORM: none; COLOR: #666666; TEXT-DECORATION: none
}
.boton {
	FONT-SIZE: 10px; COLOR: #ffffff; FONT-FAMILY: Arial, Helvetica, sans-serif; BACKGROUND-COLOR: #1766DD
}
.piedefoto {
	BACKGROUND: #ffffff; FONT-SIZE: 10px; COLOR: #888888; FONT-FAMILY: Arial, Helvetica, sans-serif 
