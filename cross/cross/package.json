{
  "name": "cross-system",
  "version": "1.0.0",
  "description": "Sistema CROSS migrado completamente a Node.js",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "create-db": "npx sequelize-cli db:create",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docker:build": "docker build -t cross-system .",
    "docker:run": "docker run -p 3000:3000 cross-system",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:restart": "pm2 restart cross-system"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "node-cron": "^3.0.3",
    "redis": "^4.6.10",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "dotenv": "^16.3.1",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "sequelize-cli": "^6.6.2"
  },
  "keywords": ["cross", "nodejs", "express", "postgresql", "workflow"],
  "author": "Amazon Q Developer",
  "license": "MIT"
}